<form method="post" action="{{ path('youtubeur_delete', {'id': youtubeur.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ youtubeur.id) }}">
    <button class="btn">Delete</button>
</form>
